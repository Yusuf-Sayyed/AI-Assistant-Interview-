    {
    "builds": [
        {
        "src": "api/index.ts",
        "use": "@vercel/node"
        },
        {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "frontend/dist"
        }
        }
    ],
    "rewrites": [
        {
        "source": "/api/(.*)",
        "destination": "/api/index.ts"
        },
        {
        "source": "/(.*)",
        "destination": "/frontend/dist/index.html"
        }
    ]
    }