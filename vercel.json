{
    "version": 2,
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/dist/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "frontend/dist/$1",
            "continue": true
        }
    ]
}